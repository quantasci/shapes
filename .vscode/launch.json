{
  "configurations": [
    {
      "name": "Debug",
      "type": "cppdbg",
      "request": "launch",      
      "program": "${command:cmake.launchTargetPath}",
      "args": ["scene_displace.txt"],
      "stopAtEntry": false,
      "cwd": "${workspaceFolder}",
      "environment": [],
      "externalConsole": false,
      "MIMode": "gdb",
      "miDebuggerPath": "/usr/bin/gdb"
      "setupCommands": [
      {
        "description": "Enable pretty-printing for gdb",
        "text": "-enable-pretty-printing",
        "ignoreFailures": true
      }
    }
  ]
}